{
  "name": "szczynk.github.io-blog",
  "version": "1.0.0",
  "description": "My personal blog about anything that i need to talk.",
  "author": "szczynk",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "pushdir": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "npm run generate && npm run pushdir"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "clipboard": "^2.0.11",
    "core-js": "^3.38.0",
    "nuxt": "^2.16.1",
    "nuxt-precompress": "^0.5.9",
    "vue": "^2.7.15",
    "vue-lazyload": "^1.3.5",
    "vue-server-renderer": "^2.7.15",
    "vue-template-compiler": "^2.7.15"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@babel/eslint-parser": "^7.25.1",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@tailwindcss/typography": "^0.5.14",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "lint-staged": "^15.2.8",
    "lqip-loader": "^2.2.1",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "push-dir": "^0.4.1",
    "responsive-loader": "^2.3.0",
    "sharp": "^0.32.6",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.4.8",
    "webpack": "^4.46.0"
  }
}
